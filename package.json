{
  "name": "api-social",
  "version": "1.0.0",
  "description": "CapsuleVerse Social API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "export NODE_ENV=development || set NODE_ENV=development && ts-node-dev --respawn ./src/index.ts",
    "prod": "export NODE_ENV=production || set NODE_ENV=production && npm run dist & pm2 start ./dist/index.js -i max",
    "db:init": "ts-node-dev src\\database\\database-cli\\index.ts -- init",
    "db:migration": "ts-node-dev src\\database\\database-cli\\index.ts",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "prepare": "cd .. && husky install api-social/.husky"
  },
  "lint-staged": {
    "src/**/*.ts": "npm run lint"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/express-rate-limit": "^6.0.0",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^18.11.17",
    "@types/node-schedule": "^2.1.0",
    "@types/request-ip": "^0.0.37",
    "@types/sequelize": "^4.28.15",
    "@types/umzug": "^2.3.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "commander": "^10.0.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "sequelize-cli": "^6.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-winston": "^4.2.0",
    "firebase-admin": "^11.8.0",
    "helmet": "^6.1.5",
    "http-errors": "^2.0.0",
    "node-schedule": "^2.1.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.2.2",
    "request-ip": "^3.3.0",
    "sequelize": "^6.31.1",
    "socket.io": "^4.5.4",
    "umzug": "^3.2.1",
    "winston": "^3.10.0",
    "xss-clean": "^0.1.1"
  }
}
